//WAP to print the multiplication table of a number between 2 and 10.
#include <stdio.h>
int main(){
    int n;
    printf("Enter the value of the number.\n");
    scanf("%d",&n);
    
    printf("The resulting multiplication table is:\n");
    for(int i=2; i<=10; i++)
    {
        printf("%d * %d = %d\n",n,i,n*i);
    }
    return 0;
}
